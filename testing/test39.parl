fun draw(states: int[100]) -> int {
    for (let x: int = 0; x < 10; x = x + 1) {
        for (let y: int = 0; y < 10; y = y + 1) {
            if (states[10 * x + y] == 1) {
                __write x, y, #ffffff;
            } else {
                __write x, y, #000000;
            }
        }
    }

    return 0;
}
